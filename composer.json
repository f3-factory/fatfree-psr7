{
    "name": "f3-factory/fatfree-psr7",
    "description": "A fat-free PHP8 implementation of PSR-7 and PSR-17",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "psr/http-message": "^1.1 || ^2.0",
        "psr/http-factory": "^1.0",
        "php-http/message-factory": "^1.0"
    },
    "license": "MIT",
    "keywords": ["psr-7", "psr-17", "http-message"],
    "authors": [
        {
            "name": "Christian Knuth // ikkez",
            "email": "ikkez0n3@gmail.com"
        }
    ],
    "provide": {
        "psr/http-message-implementation": "2.*",
        "psr/http-factory-implementation": "1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10.5 || ^11.5",
        "php-http/psr7-integration-tests": "1.*",
        "http-interop/http-factory-tests": "^2.2.0"
    },
    "autoload": {
        "psr-4": {
            "F3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\F3_PSR7\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --display-deprecations"
    }
}
